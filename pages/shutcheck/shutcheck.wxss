page {
  height: 100%;
}

.shutcheck {
  height: 100%;
  width: 100%;
  background: #ffffff;
  box-sizing: border-box;
  padding: 30rpx;
  overflow-y: auto;
}

.inner {
  -webkit-box-shadow: 0px 0px 5rpx 3rpx #cccccc;
  -moz-box-shadow: 0px 0px 5rpx 3rpx #cccccc;
  box-shadow: 0px 0px 5rpx 3rpx #cccccc;
  border-radius: 10rpx;
  font-size: 30rpx;
}

.inner .title {
  padding: 20rpx 0;
  margin: 0 15rpx;
  border-bottom: 1px #cccccc dashed;
  font-size: 32rpx;
  color: #333333;
}

.inner .content {
  padding: 10rpx 15rpx;
}

.inner .content > image {
  display: block;
  margin: 10rpx 10%;
  width: 80%;
  height: 360rpx;
}
